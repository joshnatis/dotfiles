import sys
import re

def print_match(emoji):
	if emoji in EMOJIS:
		print(EMOJIS[emoji], end="")
	else:
		print("There's no emoji named '" + emoji + "', try searching with the -s flag.")

def print_search(emoji_re):
	for key in EMOJIS.keys():
		try:
			if re.search(emoji_re, key, re.IGNORECASE):
				print(EMOJIS[key], "\t", key)
		except re.error:
				print("Your regex string '" + emoji_re + "' is malformed.")
				exit(1)

def main():
	if len(sys.argv) < 2:
		print("Usage:", sys.argv[0], "<emoji name>")
		print(" -s\tsearch the emoji table with a regex string")
		exit(1)

	query = ""
	index = 1
	search = False
	if sys.argv[1] == "-s":
		search = True
		index += 1

	for i in range(index, len(sys.argv)):
		query += sys.argv[i] + " "
	query = query[:-1] #strip space, oops

	print_search(query) if search else print_match(query)

main()
